  "id": "ru-ru",
  "pluralCat": function(n, opt_precision) {  var i = n | 0;  var vf = getVF(n, opt_precision);  if (vf.v == 0 && i % 10 == 1 && i % 100 != 11) {    return PLURAL_CATEGORY.ONE;  }  if (vf.v == 0 && i % 10 >= 2 && i % 10 <= 4 && (i % 100 < 12 || i % 100 > 14)) {    return PLURAL_CATEGORY.FEW;  }  if (vf.v == 0 && i % 10 == 0 || vf.v == 0 && i % 10 >= 5 && i % 10 <= 9 || vf.v == 0 && i % 100 >= 11 && i % 100 <= 14) {    return PLURAL_CATEGORY.MANY;  }  return PLURAL_CATEGORY.OTHER;}
});
}]);
/**
 * Shopping cart @constructor
 */
function Cart (cartName) {
    this.cartName = cartName;
    this.clearCart = false;
    this.checkoutParameters = {};
    this.items = [];

    // load items from local storage when initializing
    this.loadItems();

    // save items to local storage when unloading
    var self = this;
    $(window).unload(function () {
        if (self.clearCart) {
            self.clearItems();
        }
        self.saveItems();
        self.clearCart = false;
    });
}

/**
 * Shopping cart methods
 */
Cart.prototype = {
    
    /**
     * Load items from local storage
     */
    loadItems: function () {
        var items = localStorage != null ? localStorage[this.cartName + "_items"] : null;
        if (items != null && JSON != null) {
            try {
                var items = JSON.parse(items);
                for (var i = 0; i < items.length; i++) {
                    var item = items[i];
                    if (item.pid != null && item.name != null && item.price != null && item.quantity != null) {
                        item = new cartItem(item, item.quantity);
                        this.items.push(item);
                    }
                }
            }
            catch (err) {
                // ignore errors while loading...
            }
        }
    },
    
    /**
     * save items to local storage
     */
    saveItems: function () {
        if (localStorage != null && JSON != null) {
            localStorage[this.cartName + "_items"] = JSON.stringify(this.items);
        }
    },

    /**
     * adds an item to the cart
     */
    addItem: function (product, quantity) {
        quantity = this.toNumber(quantity);
        if (quantity != 0) {

            if (this.items.length == 0) {
                try {
                    ga('send', 'event', 'funnel', 'AddToCart');
                } catch (e) {
                    console.log(e);
                }
                try {
                    fbq('track', 'AddToCart');
                } catch (e) {
                    console.log(e);
                }
            }

            // update quantity for existing item
            var found = false;
            var specialInCart = false;
            var allowed = ["0000024", "00000034", "00000031", "00000037", "00000012", "0000016", "00000015", "00000013",
                "00000014", "50465644", "52221230", "48591631", "0000073", "00000023", "00000024", "00000025", "00000026",
                "00000067", "00000028", "00000096", "00000098", "00000100", "00000102" ];
            var specials = ["00000072", "00000076", "00000074", "00000078", "00000080", "00000082", "00000094"];
            var specialAdding = (specials.indexOf(product.pid) !== -1);

            for (var i = 0; i < this.items.length && !found; i++) {
                var item = this.items[i];
                if (item.pid == product.pid) {
                    found = true;
                    item.quantity = this.toNumber(item.quantity + quantity);
                    if (item.quantity <= 0) {
                        this.items.splice(i, 1);
                    }
                }
                if (specials.indexOf(item.pid) !== -1) {
                    specialInCart = true;
                }
            }

            // new item, add now
            if (!found) {
//                if (specialAdding && !specialInCart && this.items.length > 0) {
//                    this.items = [];
//                }
//                if (!specialAdding && specialInCart && this.items.length > 0) {
//                    if (allowed.indexOf(product.pid) !== -1) {
//                        var item = new cartItem(product, quantity);
//                        this.items.push(item);
//                    }
//
//                } else {
                    var item = new cartItem(product, quantity);
                    this.items.push(item);
//                }
            }

            // save changes
            this.saveItems();
        }
    },

    /**
     * get the total price for all items currently in the cart
     */
    getTotalPrice: function (pid) {
        var total = 0;
        for (var i = 0; i < this.items.length; i++) {
            var item = this.items[i];
            if (pid == null || item.pid == pid) {
                total += this.toNumber(item.quantity * item.price);
            }
        }
        return total;
    },

    /**
     * get the total price for all items currently in the cart
     */
    getTotalCount: function (pid) {
        var count = 0;
        for (var i = 0; i < this.items.length; i++) {
            var item = this.items[i];
            if (pid == null || item.pid == pid) {
                count += this.toNumber(item.quantity);
            }
        }
        return count;
    },

    getCountBox: function (product) {
      var countBox;
      for (var i = 0; i < this.items.length; i++) {
        var item = this.items[i];
        if (item.pid === product.pid) {
          countBox = item.quantity;
        }
      }
      return countBox;
    },

  /**
     * Clear cart
     */
    clearItems: function () {
        this.items = [];
        this.saveItems();
    },

    containsPid: function (pid) {
        for (var i = 0; i < this.items.length; i++) {
            var item = this.items[i];
            if (item.pid == pid) {
                return true;
            }
        }
        return false;
    },

    /**
     * define checkout parameters
     */
    addCheckoutParameters: function (serviceName, merchantID, options) {

        // check parameters
        if (serviceName != "PayPal" && serviceName != "Google") {
            throw "serviceName must be 'PayPal' or 'Google'.";
        }
        if (merchantID == null) {
            throw "A merchantID is required in order to checkout.";
        }

        // save parameters
        this.checkoutParameters[serviceName] = new checkoutParameters(serviceName, merchantID, options);
    },

    /**
     * Checkout
     */
    checkout: function (serviceName, clearCart) {

        // select serviceName if we have to
        if (serviceName == null) {
            var p = this.checkoutParameters[Object.keys(this.checkoutParameters)[0]];
            serviceName = p.serviceName;
        }

        // sanity
        if (serviceName == null) {
            throw "Use the 'addCheckoutParameters' method to define at least one checkout service.";
        }

        // go to work
        var parms = this.checkoutParameters[serviceName];
        if (parms == null) {
            throw "Cannot get checkout parameters for '" + serviceName + "'.";
        }
        switch (parms.serviceName) {
            case "PayPal":
                this.checkoutPayPal(parms, clearCart);
                break;
            case "Google":
                this.checkoutGoogle(parms, clearCart);
                break;
            default:
                throw "Unknown checkout service: " + parms.serviceName;
        }
    },

    /**
     * check out using PayPal
     * www.paypal.com/cgi-bin/webscr?cmd=p/pdn/howto_checkout-outside
     */
    checkoutPayPal: function (parms, clearCart) {

        // global data
        var data = {
            cmd: "_cart",
            business: parms.merchantID,
            upload: "1",
            rm: "2",
            charset: "utf-8"
        };

        // item data
        for (var i = 0; i < this.items.length; i++) {
            var item = this.items[i];
            var ctr = i + 1;
            data["item_number_" + ctr] = item.pid;
            data["item_name_" + ctr] = item.name;
            data["quantity_" + ctr] = item.quantity;
            data["amount_" + ctr] = item.price.toFixed(2);
        }

        // build form
        var form = $('<form/></form>');
        form.attr("action", "https://www.paypal.com/cgi-bin/webscr");
        form.attr("method", "POST");
        form.attr("style", "display:none;");
        this.addFormFields(form, data);
        this.addFormFields(form, parms.options);
        $("body").append(form);

        // submit form
        this.clearCart = clearCart == null || clearCart;
        form.submit();
        form.remove();
    },
    /**
     * check out using Google Wallet
     * for details see:
     * developers.google.com/checkout/developer/Google_Checkout_Custom_Cart_How_To_HTML
     * developers.google.com/checkout/developer/interactive_demo\
     */
    checkoutGoogle: function (parms, clearCart) {

        // global data
        var data = {};

        // item data
        for (var i = 0; i < this.items.length; i++) {
            var item = this.items[i];
            var ctr = i + 1;
            data["item_name_" + ctr] = item.pid;
            data["item_description_" + ctr] = item.name;
            data["item_price_" + ctr] = item.price.toFixed(2);
            data["item_quantity_" + ctr] = item.quantity;
            data["item_merchant_id_" + ctr] = parms.merchantID;
        }

        // build form
        var form = $('<form/></form>');
        // NOTE: in production projects, use the checkout.google url below;
        // for debugging/testing, use the sandbox.google url instead.
        //form.attr("action", "https://checkout.google.com/api/checkout/v2/merchantCheckoutForm/Merchant/" + parms.merchantID);
        form.attr("action", "https://sandbox.google.com/checkout/api/checkout/v2/checkoutForm/Merchant/" + parms.merchantID);
        form.attr("method", "POST");
        form.attr("style", "display:none;");
        this.addFormFields(form, data);
        this.addFormFields(form, parms.options);
        $("body").append(form);

        // submit form
        this.clearCart = clearCart == null || clearCart;
        form.submit();
        form.remove();
    },

    /**
     * Utility methods
     */
    addFormFields: function (form, data) {
        if (data != null) {
            $.each(data, function (name, value) {
                if (value != null) {
                    var input = $("<input></input>").attr("type", "hidden").attr("name", name).val(value);
                    form.append(input);
                }
            });
        }
    },

    /**
     *
     */
    toNumber: function (value) {
        value = value * 1;
        return isNaN(value) ? 0 : value;
    }
}

/**
 * Checkout parameters (one per supported payment service)
 */
function checkoutParameters(serviceName, merchantID, options) {
    this.serviceName = serviceName;
    this.merchantID = merchantID;
    this.options = options;
}

/**
 * Item in cart
 */
function cartItem(product, quantity) {
    this.pid = product.pid;
    this.name = product.name;
    this.price = product.price * 1;
    this.weight = product.weight;
    this.imageUrl = product.imageUrl;
    this.quantity = quantity * 1;
}

//----------------------------------------------------------------
// store (contains the products)
//

function Store () {
  this.products = [];
  this.ingredients = [];
}

Store.prototype.getProduct = function (sku) {
  for (var i = 0; i < this.products.length; i++) {
    if (this.products[i].sku == sku)
      return this.products[i];
  }
  return null;
};
//----------------------------------------------------------------
// product class

function Product (item) {
  this.color = item.color;
  this.name = item.name;
  this.note = item.note;
  this.text = item.text;
  this.price = item.price == undefined ? '' : item.price.trim().replace(/\s/g, '');
  this.weight = item.weight;
  this.clickText = item.click;
  this.videoUrl = item.video;
  this.imageUrl = item.image;
  this.pid = item.pid;
  this.line = item.line;
  this.best = item.best;
  this.free = item.free;
}
;(function (window) {
  var currentTime = new Date().getTime();

  function FormPhone (form) {
    this.form = form;
    this.submitBtn = form.find('button[type="submit"]');
    this.phoneInput = form.find('.input-phone');

    this.pendingMsg = form.find('#pending');
    this.defaultMsg = form.find('#default');
    this.succesTime = form.find('#success-time');
    this.succesTimeResult = form.find('#success-time-result');
    this.succesWait = form.find('#success-wait');

    this.isSubmiting = false;
    this.init();
  }

  FormPhone.prototype = {
    submit: function () {
      if (!$(this.phoneInput).inputmask('isComplete')) {
        return false;
      }

      try {
        var duration = new Date().getTime()  - currentTime;
        ga('send', 'event', 'phone request', 'submit', duration);
      } catch (e) {
        console.error(e)
      }

      this.isSubmiting = true;
      var data = this.form.serialize();
      var that = this;

      $.ajax({
        type: 'POST',
        url: 'public/themes/edoque/mail.chunk.php',
        data: data, 
        beforeSend: function () {
          that.disableSubmit();
          that.pendingMsg.show();
          that.defaultMsg.hide();
          that.succesTimeResult.hide();
          that.succesWait.hide();
        },
        success: function(data){ 
          that.pendingMsg.hide();
          if (data.error) {
            form.find('.header-phone-text #error').show();
          } else {
            that.checkTimeZone();
          }
        },
        error: function (xhr, ajaxOptions, thrownError) {
          console.log(xhr.status);
          console.log(thrownError);
        }            
      });
      
      return false;
    },
    disableSubmit: function () {
      this.submitBtn.attr('disabled', 'disabled');
    },
    unDisableSubmit: function () {
      this.submitBtn.attr('disabled', false);
    },
    checkPhone: function () {
      if ( $(this.phoneInput).inputmask('isComplete') ) {
        !this.isSubmiting && this.unDisableSubmit();
      } else {
        this.disableSubmit();
      }
    },
    checkTimeZone: function () {
      var intervalHandle, secondsRemaining;
      var that = this;
      
      if (isWorkingTime()) {
        startCountdown();
      } else {
        that.succesWait.show();
        that.isSubmiting = false;
        that.unDisableSubmit();
      }

      function tick(){
        var timeDisplay = document.getElementById("time");
        timeDisplay.innerHTML = secondsRemaining;

        // stop is down to zero
        if (secondsRemaining === 0){
          clearInterval(intervalHandle);
          that.succesTime.hide();
          that.succesTimeResult.show();
          that.isSubmiting = false;
          that.unDisableSubmit();
        }

        secondsRemaining--;
      }

      function startCountdown(){
        that.succesTime.show();
        secondsRemaining = 120;
        
        if (intervalHandle) {
          clearInterval(intervalHandle);
        }
        intervalHandle = setInterval(tick, 1000);
      }
    },
    init: function () {
      this.form.submit(this.submit.bind(this));
      this.phoneInput.on('keyup', this.checkPhone.bind(this));
      this.phoneInput.on('focus', this.checkPhone.bind(this));
      this.phoneInput.on('blur', function () {
        !this.isSubmiting && this.unDisableSubmit();
      }.bind(this));

      if (!isWorkingTime()) {
        this.defaultMsg.hide();
      }
    }
  };

  function isWorkingTime() {
    var d = new Date();
    var mskHours = d.getUTCHours() + 3;

    return (mskHours >= 10 && mskHours <= 21);

  }

  window.FormPhone = FormPhone;
}(window));
setTimeout(function () {
  var cities = {
    msk: {
      title: 'Москва',
      url: 'http://msk.edoque.ru/'
    },
    spb: {
      title: 'Санкт-Петербург',
      url: 'http://edoque.ru/'
    }
  };

  var hidePopup = function () {
    $('#popup').fadeOut('slow');
    $('html').removeClass('show-popup');
  };

  var showPopup = function () {
    $('#popup').fadeIn('slow');
    $('html').addClass('show-popup');
  };

  var selectCity = function (cityCode) {
    hidePopup();
    $.cookie('city', cityCode, {expires: 365, domain: 'edoque.ru'});
    $('#city')[0].innerHTML = cities[cityCode].title;
    window.location.href = cities[cityCode].url;
  };

  $('#msk').click(function () {
    selectCity('msk');
  });

  $('#spb').click(function () {
    selectCity('spb');
  });

  $('#city').click(showPopup);

  if (!$.cookie('city')) {
    showPopup();
  } else {
    $('#city')[0].innerHTML = cities[$.cookie('city')].title;
  }

  $("#phone").inputmask({
    mask: '+7 (999) 999-99-99',
    showMaskOnHover: false
  });

  $('.header-fruit-box-title').click(function () {
    $('html, body').animate({
      scrollTop: $('.jsFruitBox:first').offset().top
    }, 1000);
  });

  $(".menu-wrapper a").click(function () {
    var elementClick = $(this).attr("href"),
      destination = $(elementClick).offset().top;
    $('html,body').animate( { scrollTop: destination }, 500);
  });

  if ($('html').hasClass('desktop')){
    $("#scroll-top").hide();
    $(window).scroll(function () {
      if ($(this).scrollTop() > 200) {
        $('#scroll-top').fadeIn(1000);
      } else {
        $('#scroll-top').fadeOut(500);
      }
    });
    $('#scroll-top').click(function () {
      $('body,html').animate({
        scrollTop: 0
      }, 500);
    });
  }

  if(/iP(hone|ad)/.test(window.navigator.userAgent)) {
    var buttons = document.querySelectorAll('button'),
      elements = $(".touch"),
      emptyFunctionButtons = function() {};
    emptyFunctionElements = function() {};
    for (var i = 0; i < buttons.length; i++) {
      buttons[i].addEventListener('touchstart', emptyFunctionButtons, false);
    }
    for (var i = 0; i < elements.length; i++) {
      elements[i].addEventListener('touchstart', emptyFunctionElements, false);
    }
  }

},1000);
'use strict';

angular.module('Edoque', [
  'ui.router',
  'selectize',
  'ngAnimate'
])

.config(['$stateProvider', '$urlRouterProvider', function($stateProvider, $urlRouterProvider) {
  $stateProvider
    .state('edoque', {
      templateUrl: 'public/themes/edoque/views/index.view.html',
      controller: 'EdoqueCtrl',
      resolve: {
        blocks: ['$http', function ($http) {
          return $http.get('/public/themes/edoque/blocks.json').then(function (response) {
            return response.data;
          });
        }],
        boxes: ['$http', function ($http) {
          return $http.get('public/themes/edoque/boxes.json').then(function (response) {
            return response.data;
          });
        }],
        ingredients: ['$http', function ($http) {
          return $http.get('public/themes/edoque/ingredients.json').then(function (response) {
            return response.data;
          });
        }]
      }
    })
    .state('edoque.store', {
      url: '/',
      templateUrl: 'public/themes/edoque/views/store.view.html',
      controller: 'StoreCtrl'
    })
    .state('edoque.cart', {
      url: '/cart',
      controller: 'CartCtrl',
      templateUrl: 'public/themes/edoque/views/cart.view.html',
      resolve: {
        deliveryTypes: ['$http', function ($http) {
          return $http({
            url: 'http://api.edoque.ru/delivery_types.php',
            method: 'GET',
            cache: true
          }).then(function (response) {
            return response.data;
          });
        }]
      }
    })
    .state('edoque.success', {
      url: '/success',
      templateUrl: 'public/themes/edoque/views/success.view.html',
      onEnter: function ($location, $anchorScroll) {
        $location.hash('message');
        $anchorScroll();
      }
    })
    .state('edoque.error', {
      url: '/fail',
      templateUrl: 'public/themes/edoque/views/error.view.html',
      onEnter: function ($location, $anchorScroll) {
        $location.hash('message');
        $anchorScroll();
      }
    });

  $urlRouterProvider.otherwise('/');
}])

.factory('DataService', function () {
  var myStore = new Store();
  var myCart = new Cart('EdoqueStore');

  return {
    store: myStore,
    cart: myCart
  };
})

.controller('EdoqueCtrl', ['$http', '$scope', '$state', 'DataService', 'blocks', 'boxes', 'ingredients',
  function($http, $scope, $state, DataService, blocks, boxes, ingredients) {

  console.log('Welcome to Edoque!');

  $scope.store = DataService.store;
  $scope.cart = DataService.cart;
  $scope.isMobile = device.mobile();

  $scope.blocks = blocks;
  $scope.$state = $state;

  $scope.goFromBasket = goFromBasket;

  boxes.forEach(function (box) {
    DataService.store.products.push(new Product(box));
  });

  ingredients.forEach(function (item) {
    DataService.store.ingredients.push(new Product(item));
  });

  function goFromBasket () {
    var state = $state.current.name == 'edoque.cart'
      ? 'edoque.store'
      : 'edoque.cart';

    $state.go(state);
  }
}])

.controller('StoreCtrl', ['$scope', function ($scope) {

  $scope.sendMailChimp = sendMailChimp;

  var errObj = {
    'ValidationError': 'Возможно, вы указали неверный email, если вы уверены, что он правильный, сообщите об этой ошибке по адресу omnomnom@edoque.ru',
    'User_InvalidAction': '(User_InvalidAction) пожалуйста, сообщите об этой ошибке по адресу omnomnom@edoque.ru',
    'User_UnderMaintenance': 'увы, сейчас на сервере идут работы, напишите нам по адресу omnomnom@edoque.ru',
    'Too_Many_Connections': '(Too_Many_Connections) пожалуйста, сообщите об этой ошибке по адресу omnomnom@edoque.ru',
    'User_InvalidRole': '(User_InvalidRole) пожалуйста, сообщите об этой ошибке по адресу omnomnom@edoque.ru',
    'User_Disabled': '(User_Disabled) пожалуйста, сообщите об этой ошибке по адресу omnomnom@edoque.ru',
    'Invalid_ApiKey': '(Invalid_ApiKey) пожалуйста, сообщите об этой ошибке по адресу omnomnom@edoque.ru'
  };

  function sendMailChimp (e) {
    e.preventDefault();
    var form = $(e.target);

    if (form.hasClass('valid-email')) {
      var data = form.serialize();
      $.ajax({
        type: 'POST',
        url: 'public/themes/edoque/mc.php',
        dataType: 'json',
        data: data,
        beforeSend: function(data) {
          form.find('button[type="submit"]').attr('disabled', 'disabled');
        },
          success: function(data){
              console.log( data );
              console.log( data.status );
              if (data.status == 400) form.find('#error').append( data.title ).delay(15000).fadeOut();
              if (data.status == 'subscribed') form.find('#success').show().delay(5000).fadeOut();
          },
        error: function (xhr, ajaxOptions, thrownError) {
          console.log(xhr.status);
          console.log(thrownError);
        },
        complete: function(data) {
          form.find('button[type="submit"]').prop('disabled', false);
        }
      });
    }

    return false;
  }
}])

.controller('CartCtrl', ['$scope', '$http', '$timeout', '$filter', 'DataService', '$state', 'deliveryTypes',
    function ($scope, $http, $timeout, $filter, DataService, $state, deliveryTypes) {

  console.log('Welcome to Cart!');
    try {
        ga('send', 'event', 'funnel', 'InitiateCheckout');
    } catch (e) {
        console.log(e);
    }
  try {
    fbq('track', 'InitiateCheckout');
  } catch (e) {
    console.log(e);
  }

  if ($("html").hasClass('mobile')) {
    $(window).scroll(function(){
      if (($('#backToShop').length)) {
        amountInViewport($('#basketAmount')[0]);
      }
    });
  }

  var specialOrder = isSpecialOrder();

  var D = $filter('date');
  var d = new Date();

  var mskHours = d.getUTCHours() + 3;

  var base; // if time <= 21 hours -> tomorrow -> else day after tomorrow

  if (mskHours < 21 || mskHours >= 24) {
    base = new Date(d.getFullYear(), d.getMonth(), d.getDate() + 1);
  } else {
    base =new Date(d.getFullYear(), d.getMonth(), d.getDate() + 2);
  }

  if (specialOrder) {
    base = new Date(2016, 1, 13, 0, 0, 0, 0);
    var day_1 = new Date(base.getFullYear(), base.getMonth(), base.getDate());
    var day_2 = new Date(base.getFullYear(), base.getMonth(), base.getDate() + 1);
//    var day_3 = new Date().setDate(day_1.getDate() + 2);

      $scope.deliveryDay = [
          { id: D(day_1, 'yyyy-MM-dd'), title: D(day_1, 'EEEE, d MMMM') },
          { id: D(day_2, 'yyyy-MM-dd'), title: D(day_2, 'EEEE, d MMMM') },
      ];
  } else {
      var day_1 = new Date(base.getFullYear(), base.getMonth(), base.getDate());
      var day_2 = new Date(base.getFullYear(), base.getMonth(), base.getDate() + 1);
      var day_3 = new Date(base.getFullYear(), base.getMonth(), base.getDate() + 2);

      if (base.getMonth() == 2 && base.getDate() <= 6) {
          $scope.deliveryDay = [ ];
          for (var i = base.getDate(); i <= 9; i++) {
              console.log('day '+i);
              var day = new Date(base.getFullYear(), base.getMonth(), i );
              $scope.deliveryDay.push( { id: D(day, 'yyyy-MM-dd'), title: D(day, 'EEEE, d MMMM') } );
          }
      } else {
          $scope.deliveryDay = [
              { id: D(day_1, 'yyyy-MM-dd'), title: D(day_1, 'EEEE, d MMMM') },
              { id: D(day_2, 'yyyy-MM-dd'), title: D(day_2, 'EEEE, d MMMM') },
              { id: D(day_3, 'yyyy-MM-dd'), title: D(day_3, 'EEEE, d MMMM') }
          ];
      }

  }

  // we have different delivery times
  var deliveryTimeMainZone = [
      {id: '09:00-14:00', title: '09:00-14:00'},
      {id: '14:00-18:00', title: '14:00-18:00'},
      {id: '18:00-23:00', title: '18:00-23:00'},
    {id: 'time-msk', title: 'К точному времени (750 руб)'}
  ];

  var deliveryTimeMainZoneWeekend = [
    {id: '09:00-14:00', title: '09:00-14:00'},
    {id: '14:00-18:00', title: '14:00-18:00'},
    {id: '18:00-23:00', title: '18:00-23:00'},
    {id: 'time-msk', title: 'К точному времени (750 руб)'}
  ];

  var deliveryTimeFarZonesWeekend = [
    {id: '09:00-14:00', title: '09:00-14:00'},
    {id: '14:00-18:00', title: '14:00-18:00'},
    {id: '18:00-23:00', title: '18:00-23:00'}
  ];

  var deliveryTimeFarZones = [
      {id: '09:00-14:00', title: '09:00-14:00'},
      {id: '14:00-18:00', title: '14:00-18:00'},
      {id: '18:00-23:00', title: '18:00-23:00'}
  ];

  var selfDeliveryTime = [
    {id: '14:00-21:00', title: '14:00-21:00'}
  ];

    var deliveryTime31Dec = [
        {id: '10:00-16:00', title: '10:00-16:00'}
    ];

    var deliveryTime8Mar = [
        {id: '09:00-14:00', title: '09:00-14:00'},
        {id: '14:00-18:00', title: '14:00-18:00'},
        {id: '18:00-23:00', title: '18:00-23:00'},
//        {id: 'time-msk', title: 'К точному времени (750 руб)'}
    ];

    var deliveryTime1Sep = [
        {id: '08:00-09:00', title: '08:00-09:00'},
        {id: '09:00-10:00', title: '09:00-10:00'}
    ];


//    var deliveryTime8Mar = [
//        {id: '09:00-15:00', title: '09:00-15:00'},
//        {id: '15:00-22:00', title: '15:00-22:00'}
//    ];

  $scope.order = {
    is_gift: false,
    date: $scope.deliveryDay[0].id,
    time_custom: null,
    delivery_cost: 0,
    delivery: 'mkad',
    discount_coupon: 0,
    do_not_call: false
  };

  if (specialOrder) {
    $scope.deliveryTime = is31ofDec($scope.order.date)
      ? deliveryTime31Dec
      : deliveryTimeMainZoneWeekend;
  } else {
    $scope.deliveryTime = is8ofMarch($scope.order.date) ? deliveryTime8Mar : isWeekend($scope.order.date)
      ? deliveryTimeMainZoneWeekend
      : deliveryTimeMainZone;
  }

  $scope.order.time = $scope.deliveryTime[0].title;

  $scope.order.delivery_cost = deliveryTypes[$scope.order.delivery].defaultCost;
  $scope.deliveryPlace = [];

  // get only needed delivery types
  var allowed_keys = [ 'mkad', '1sept-msk' ];
  Object.keys(deliveryTypes).forEach(function (key) {
    if (allowed_keys.indexOf(key) !== -1) {
      $scope.deliveryPlace.push({
        id: key,
        title: deliveryTypes[key].name,
        price: deliveryTypes[key].defaultCost
      });
    }
  });

  // check items count
  // set zero coupon if items cost less than MIN
  var coupon_min_cost = null;
  var _lastDeliveryPrice = null;
  $scope.$watch(function () {
    return DataService.cart.getTotalCount();
  }, function (now, past) {

      invalidateDeliveryCost();
  });

  $scope.config = {
    maxItems: 1,
    valueField: 'id', // id ?
    labelField: 'title',
    sortField: 'title',
    onChange: function (index) {

      if (index == 'time-msk') {
        $scope.showCustomTime = true;
        $timeout(function() {
          angular.element('#order-custom-time')[0].focus();
        });
      } else {
        $scope.showCustomTime = false;
        $scope.order.time_custom = null;
      }

      invalidateDeliveryCost();
      $scope.$apply();
    }
  };

  $scope.configDate = {
    maxItems: 1,
    valueField: 'id',
    labelField: 'title',
    sortField: 'id',
    onChange: function (date) {
      onChangeDelivery($scope.order.delivery);
    }
  };

  $scope.configDelivery = {
    maxItems: 1,
    valueField: 'id',
    labelField: 'title',
    sortField: 'price',
    onChange: onChangeDelivery
  };

  function amountInViewport(element) {
    var top = element.offsetTop;
    var height = element.offsetHeight;

    while(element.offsetParent) {
      element = element.offsetParent;
      top += element.offsetTop;
    }

    if (top >= window.pageYOffset && (top + height) <= (window.pageYOffset + window.innerHeight)) {
      $('#backToShop').fadeOut();
    } else {
      $('#backToShop').fadeIn();
    }
  }

  function onChangeDelivery (key) {
    var delivery = deliveryTypes[key];
    var weekend = isWeekend($scope.order.date);
    var specialOrder = isSpecialOrder();

    // change coupon, time
    // that depends on dilevery
    if (delivery.code == '1sept-msk') {
      $scope.deliveryTime = deliveryTime1Sep;
    } else if (delivery.code == 'self-delivery') {
      $scope.deliveryTime = selfDeliveryTime;
    } else if (delivery.code == 'mkad') {
        if (specialOrder) {
            $scope.deliveryTime = is31ofDec($scope.order.date) ? deliveryTime31Dec : deliveryTimeMainZoneWeekend;
        } else {
            $scope.deliveryTime = is8ofMarch($scope.order.date) ? deliveryTime8Mar : (weekend
                ? deliveryTimeMainZoneWeekend
                : deliveryTimeMainZone);
        }
    } else {
        if (specialOrder) {
            $scope.deliveryTime = is31ofDec($scope.order.date) ? deliveryTime31Dec : deliveryTimeFarZonesWeekend;
        } else {
            $scope.deliveryTime = is8ofMarch($scope.order.date) ? deliveryTime8Mar : (weekend
                ? deliveryTimeFarZonesWeekend
                : deliveryTimeFarZones);
        }
    }

    $scope.showCustomTime = false;
    $scope.order.time = $scope.deliveryTime[0].title;
    invalidateDeliveryCost();

    $scope.$apply();
  }

    function invalidateDeliveryCost () {
        // remove any coupon
        $scope.order.discount_coupon = 0;
        $scope.coupon = null;
        // delivery_cost depends on amount, delivery type and custom time
        if ( $scope.showCustomTime ) {
            $scope.order.delivery_cost = deliveryTypes['time-msk'].defaultCost;
        } else if ($scope.order.delivery == 'mkad' && getProductPrice() >= 4000) {
            $scope.order.delivery_cost = 0;
        } else {
            // order.delivery=time &
            // order.delivery=self-delivery included
            $scope.order.delivery_cost = deliveryTypes[$scope.order.delivery].defaultCost;
        }
    }

  $scope.sendOrder = sendOrder;
  $scope.getCurrentPrice = getCurrentPrice;
  $scope.checkCoupon = checkCoupon;
  $scope.isEdoquePayment = isEdoquePayment;

  function checkCoupon (coupon) {
    $http({
      url: '/public/themes/edoque/coupons.php',
      method: 'POST',
      data: {
        coupon: coupon
      }
    }).success(function (response) {
      console.log(response);
      if (response.correct) {
        if ($scope.showCustomTime ||
            (getProductPrice() >= 4000 && $scope.order.delivery == 'mkad')) {
          return;
        }

        var min_cost = response.coupon.min_cost || 0;

        if ($scope.order.delivery != 'self-delivery' && getProductPrice() >= min_cost) {
          $scope.order.discount_coupon = parseNumber(response.coupon.price) || 349;
          coupon_min_cost = parseInt(response.coupon.min_cost || 0);
        }
      } else {
        $scope.order.discount_coupon = 0;
      }
    }).error(function (response) {
      console.log(response);
      $scope.order.discount_coupon = 0;
    });
  }

  function parseNumber (number) {
    var result = parseInt(number);

    if (isNaN(result)) {
      result = 0;
    }

    return result;
  }

  // price without delivery cost
  function getProductPrice () {
    return DataService.cart.getTotalPrice();
  }

  // total price, with delivery
  function getCurrentPrice () {
    return getProductPrice() +
      parseNumber($scope.order.delivery_cost) -
      parseNumber($scope.order.discount_coupon);
  }

  function isEdoquePayment (payment) {
    return ['cash', 'bank-transfer'].indexOf(payment) !== -1;
  }

  function isWeekend (string) {
    var day = new Date(string).getDay();
    return (day == 6) || (day == 0);
  }

    function is31ofDec (string) {
        var day = new Date(string).getDate();
        var m = new Date(string).getMonth();
        return (day == 31 && m == 11);
    }
    function is8ofMarch (string) {
        var day = new Date(string).getDate();
        var m = new Date(string).getMonth();
        return (day == 8 && m == 2);
    }

    function isSpecialOrder () {
        return (DataService.cart.containsPid("00000072") ||
            DataService.cart.containsPid("00000076") ||
            DataService.cart.containsPid("00000074") ||
            DataService.cart.containsPid("00000078") ||
            DataService.cart.containsPid("00000080") ||
            DataService.cart.containsPid("00000082") ||
            DataService.cart.containsPid("00000082"))
    }

  function sendOrder (isValid, $event) {
    $scope.submitted = true;

    if (!isValid) {
      return;
    }

    var data = $scope.order;
    data.items = DataService.cart.items;
    data.phone_plain = data.phone.replace(/\D/g, '');

    if (data.time_custom) {
      data.time = data.time_custom;
    }

    if ($scope.coupon && $scope.order.discount_coupon) {
      data.coupon = $scope.coupon;
    }

    switch ($scope.order.paymentType) {
      case 'cash':
        data.payment = 'cash'; break;
      case 'bank-transfer':
        data.payment = 'bank-transfer'; break;
      default:
        data.payment = 'bank-card'; break;
    }

    $(".sendOrder").attr('disabled', true);

    $http({
      method: 'POST',
      //url: 'public/themes/edoque/test.php',
      url: '//api.edoque.ru/order.php',
      data: data,
      withCredentials: true
    }).success(function (orderNumber) {

    try {
        ga('require', 'ecommerce', 'ecommerce.js');
        ga('ecommerce:addTransaction', {
            'id': orderNumber,             // ID транзакции
            'affiliation': 'Edoque Москва',   // Название магазина
            'revenue': getCurrentPrice(),   // Общая стоимость заказа
            'shipping': parseNumber($scope.order.delivery_cost)  // Стоимость доставки
        });

        var items = DataService.cart.items;
        for (var i = 0; i < items.length; i++) {
            console.log('ecommerce:addItem ',items[i].name);
            ga('ecommerce:addItem', {
                'id': orderNumber,            // ID транзакции
                'name': items[i].name,        // Название товара
                'sku': items[i].pid,        // Название товара
                'price': items[i].price,      // Стоимость товара
                'quantity':  items[i].quantity  // Количество товара
            });
        }
        ga('ecommerce:send');
    } catch (e) {
        console.log(e);
    }

      try {
        fbq('track', 'Purchase', {value: getCurrentPrice(), currency: 'RUB'});
      } catch (e) {
        console.log(e);
      }

      // if yandex payment -> add order number -> submit form
      if (!isEdoquePayment($scope.order.paymentType)) {
        var input = document.createElement('input');
        input.type = 'hidden';
        input.name = 'orderNumber';
        input.value = orderNumber;
        $event.target.appendChild(input);

        $event.target.action = 'http://money.yandex.ru/eshop.xml';
        $event.target.submit();
      } else {
        $state.transitionTo('edoque.success');
      }
      DataService.cart.clearItems();
        ga('ecommerce:clear');
      $(".sendOrder").attr('disabled', false);
    }).error(function (error) {
      $state.transitionTo('edoque.error');
    });

  }
}])

.run(['$rootScope', '$location', '$anchorScroll', '$timeout', function($rootScope, $location, $anchorScroll, $timeout) {
  $rootScope.$on('$stateChangeStart', function(event, toState, toParams, fromState, fromParams) {
    if($location.hash()) $anchorScroll();
  });
}])

.directive('block', ['$compile', function ($compile) {
  return {
    restrict: 'A',
    link: function (scope, element, attr) {
      element.html(scope.blocks[attr.block]);
      $compile(element.contents())(scope);
    }
  };
}])

.directive('mmenu', function () {
  return {
    restrict: 'A',
    link: function (scope, element, attr) {
      var $menu = $(element);
      var $menuLinks = $(element).find('a');

      $menu.mmenu().on('click', function(e){
        e.preventDefault();

        $menuLinks.each(function () {
          $(this).removeClass('active');
        });

        $(this).addClass('active');

        $menu.trigger('close.mm');
      });

      var API = $(element).data( "mmenu" );

      $('.menu-icon').click(function() {
        API.open();
      });
    }
  };
})

.directive('slide', function () {
  return {
    link: function (scope, element, attr) {
      if (scope.$last === true) {
        $('.slider').bxSlider({
          slideWidth: 200,
          minSlides: 2,
          maxSlides: 4,
          slideMargin: 30,
          infiniteLoop: false,
          hideControlOnEnd: true
        });
      }
    }
  };
})

.directive('phone', function () {
  return {
    link: function (scope, element, attr) {
      $(element).inputmask({
        mask: '+7 (999) 999-99-99',
        showMaskOnHover: true
      });
    }
  };
})

.directive('customTime', function () {
  return {
    link: function (scope, element, attr) {
      $(element).inputmask({
        mask: '99:99',
        showMaskOnHover: false
      });
    }
  };
})

.directive('edoqueCart', function () {
  return {
    link: function (scope, element, attr) {
      scope.$watch(function () {
        return scope.cart.getTotalCount();
      }, function (curr, prev) {
        element.addClass('item-added');

        setTimeout(function () {
          element.removeClass('item-added');
        }, 500);
      });
    }
  };
})

.directive('edoqueValidEmail', function () {
  return {
    link: function (scope, element, attr) {
      var emailWrapper = element.parent();

      element.on('keyup', function() {
        var text = $(this).val();

        if (text) {
          if (isValidEmailAddress(text)){
            emailWrapper.addClass('valid-email');
          } else{
              emailWrapper.removeClass('valid-email');
          }
        }
      });

      function isValidEmailAddress(email){
        var pattern = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);

        return pattern.test(email);
      }
    }
  };
})

.directive('edoqueHeaderPhone', function () {
  return {
    priority: 1000,
    link: function (scope, element, attr) {
      new FormPhone( $(element).find('form') );
    }
  };
})

.directive('edoqueAddIngredient', ['$timeout', function ($timeout) {
  return {
    restrict: 'A',
    link: function (scope, element, attr) {
      element.click(function () {
        element.text('Добавлено');
        $timeout(function () {
          element.text('Добавить');
        }, 5000);
      });
    }
  };
}])

.directive('edoqueBox', ['$timeout', function ($timeout) {
  return {
    restrict: 'A',
    link: function (scope, element, attr) {
      var addButton = $(element).find('.add-to-cart');
      var openButton = $(element).find('.open-cart');

      addButton.click(function () {
        if (!$(element).hasClass('free-choice')) {
          addButton.addClass('hide');
          openButton.removeClass('hide');
          $timeout(function () {
            addButton.removeClass('hide');
            openButton.addClass('hide');
          }, 5000);
        }
      });

      var circleImage = element.find('.fruit-box-more');

      if ($('html').hasClass('desktop')){
        var isHover = false;
        circleImage.hover(function (){
          isHover = true;
          wrapperVideo = $(this);
          video = wrapperVideo.find(".fruit-box-video")[0];
          if (video && video.paused) {
            video.play();
            video.onended = function() {
                // check video ended
                if (isHover && video.duration <= video.currentTime) {
                    showBackCard();
                }
            }
          }
          // check if video loaded
          if (video.readyState != 0) {
              wrapperVideo.addClass("isVideoActive");
          } else {
            // wait video loading
            function checkLoad() {
                if (video.readyState > 2 && isHover) {
                    wrapperVideo.addClass("isVideoActive");
                } else {
                    setTimeout(checkLoad, 100);
                }
            }
            checkLoad();
          }

        }, function () {
          isHover = false;
          wrapperVideo = $(this);
          wrapperVideo.removeClass("isVideoActive");
          video = wrapperVideo.find(".fruit-box-video")[0];
          if (video && !video.paused && video.readyState != 0) {
              video.currentTime = 0;
          }
        });
      }

      var backCard = element.find('.fruit-box-back');
      var frontCard = element.find('.fruit-box-front');

      circleImage.on('click', showBackCard);
      backCard.on('click', showFrontCard);

      function showBackCard (e) {
        element.addClass('flip-toggle');
        backCard.css('opacity', 1)

        setTimeout (function(){
            frontCard.css('opacity', 0)
        }, 100);
      }

      function showFrontCard (e) {
        if ($(e.target).hasClass('default-button')){
          return false;
        }

        element.removeClass('flip-toggle');

        setTimeout(function() {
            frontCard.css('opacity', 1)
        }, 100);

        backCard.css('opacity', 0)
      }
    }
  };
}]);
